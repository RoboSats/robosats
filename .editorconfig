# .editorconfig http://editorconfig.org/

root = true

[*]
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true

# 4 space indentation
[*.py]
indent_style = space
indent_size = 4


# 2 space indentation
[*.js]
indent_style = space
indent_size = 2